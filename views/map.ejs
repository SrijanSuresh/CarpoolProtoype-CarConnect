<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Map</title>
    <style>
        html{
            box-sizing: border-box;
            font-size: 100%;
        }

        *, *::before, *::after{
            box-sizing: inherit; 
        }

        h1{
            font-size: 3em;
        }

        body{
            margin: 0;
            padding: 0;
        }

        #map{
            height: 60vh;
        }

    </style>
</head>
<body>
    
    <h1>Testing out this map</h1>

    <div id="map"></div>
    <script src = "../mapscript.js"></script>
    <script 
        src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBuStdDeWkNTyDN8ddLyj4vHd3gqBvlrJI&map_ids=e31cee457f27fcf7&callback=initMap">
    </script>
</body>
</html>


<script>
    function initMap() {
        map = new google.maps.Map(document.getElementById('map'), {
            center: {lat: -34.397, lng: 150.644},
            zoom: 8,
            mapId: 'e31cee457f27fcf7' // Make sure this mapId is correct
        });
    }
</script>
